skipped_resolutions:
  packages:
    - name: elt-patches
      category: app-portage

    - name: eselect-opengl
      category: app-eselect

    - name: binutils-apple
      category: sys-devel

    - name: gcc-apple
      category: sys-devel

  build_categories:
    - catX

  categories:
    - virtual
#    - cat2


replacements:
  runtime_deps:
    packages:
      - from:
          name: gcc
          category: sys-devel
        to:
          name: base-gcc
          category: sys-devel



#include_files:
#  - artefacts/file1.yml

build_template_file: templates/build-pkg.yaml.tmpl

# Define the template file to use for build *-portage
# packages.
build_portage_template_file: templates/build-pkg-portage.yaml.tmpl

artefacts:
- tree: multi-arch/packages

# Inside single artefact section is possible define the list of
# use flags to enable or disable.
# uses:
#   disabled:
#     - use1
#   enabled:
#     - use2
#
# To avoid cycle on solver we need ignore buildtime dependencies and
# to use directly build-seed layer or other. This option permits to
# ignore build deps elaborated.
# ignore_build_deps: true

  override_version: "0.1.0+1"
  mutations:
    runtime_deps:
      # Define a list of requirements to add on the package.
      packages:
        - name: foo
          category: cat
    # Define the list of use flags to add
    uses:
      - x86_32
  packages:
    - net-libs/libssh2
#- sys-devel/gcc:9.3.0
#    - app-emulation/lxd
#    - app-emulation/lxc

# Define the list of the files generated by Funtoo reposcan tool.
reposcan_sources:
#  - source1.json
  - /tmp/kit_cache/gentoo-master
#  - /tmp/kit_cache/geaaru_overlay-master

# Disable use of category with slot as requires. Default is without slot.
reposcan_requires_slot: true

reposcan_contraints:
  packages:
    - dev-lang/python:3.7
    - sys-devel/gcc:9.3.0

reposcan_disabled_use_flags:
  - test
  - qt5
  - emacs
  - doc
  - python_single_target_python3_6
  - python_single_target_python3_8
  - python_single_target_python3_9
  - savedconfig

reposcan_disabled_keywords:
  - ~amd64

# Define layers packages to use as provides of singles packages in build phase.
build_layers:
  - layer:
      name: build-core
      category: layer
    packages:
      - dev-lang/python
      - sys-devel/gcc

